package com.springcore;

public class Test {
	private int id;
	private String Name;

	public Test(int id, String name) {
		super();
		
		System.out.println("Para Constructror");
		this.id = id;
		Name = name;
	}

	@Override
	public String toString() {
		return "Test [id=" + id + ", Name=" + Name + "]";
	}

}
